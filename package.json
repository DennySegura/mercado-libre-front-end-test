{
  "name": "mercado-libre-front-end-test",
  "version": "1.0.0",
  "description": "Prueba tecnica front para mercado libre argentina",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "lint": "eslint --fix .",
    "test": "jest",
    "test-watch": "jest --watch",
    "cp-to-dist": "gulp copy-to-dist",
    "mkdir-dist": "rm -rf dist && mkdir dist",
    "tsc": "tsc -p ./tsconfig.json",
    "ts-node": "ts-node --inspect=5858 ./src/index.ts",
    "nodemon": "nodemon --exec 'ts-node --cache-directory .tscache' ./src/index.ts",
    "build": "npm run mkdir-dist && npm run tsc && npm run cp-to-dist",
    "dev": "export NODE_ENV=development && clear && npm run mkdir-dist && npm run cp-to-dist && npm run nodemon",
    "pr": "export NODE_ENV=release && clear && npm run mkdir-dist && npm run cp-to-dist && npm run nodemon",
    "start-pr": "export NODE_ENV=release && npm run build && npm run start",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-healthcheck": "^0.1.0",
    "http-status-codes": "^1.3.0",
    "i18n": "^0.8.3",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.35",
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-serve-static-core": "^4.11.1",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.107",
    "@types/method-override": "0.0.31",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.18",
    "@types/supertest": "^2.0.4",
    "@types/winston": "^2.3.9",
    "gulp": "^3.9.1",
    "jest": "^22.4.2",
    "nodemon": "^1.17.3",
    "supertest": "^3.0.0",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DennySegura/mercado-libre-front-end-test.git"
  },
  "keywords": [
    "mercado-libre",
    "react",
    "nodejs",
    "server",
    "front-end",
    "back-end",
    "Argentina",
    "best-practices"
  ],
  "author": "Denny Segura <dennysegura39@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DennySegura/mercado-libre-front-end-test/issues"
  },
  "homepage": "https://github.com/DennySegura/mercado-libre-front-end-test#readme"
}
